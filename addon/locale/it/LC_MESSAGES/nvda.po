msgstr "Nessuna informazione per l'aggiornamento"

#: addon\GlobalPlugins\UnigramPlus\__init__.py:127
msgid "Open UnigramPlus settings window"
msgstr "Apri la finestra delle impostazioni di UnigramPlus"

#: addon\GlobalPlugins\UnigramPlus\__init__.py:134
